                <ImageView android:id="@+id/imageVectorThree"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_20pxh"
                    android:layout_marginBottom="@dimen/_6pxv"
                    android:layout_marginStart="@dimen/_57pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="@dimen/_27pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_vector"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_vector"/>
                <ImageView android:id="@+id/imageIconbookc"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_30pxh"
                    android:layout_marginStart="@dimen/_85pxh"
                    android:layout_marginTop="@dimen/_2pxv"
                    android:layout_width="@dimen/_21pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_iconbookc"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_iconbookc"/>

                <ImageView
                    android:id="@+id/imageVectorFour"
                    android:layout_width="27dp"
                    android:layout_height="16dp"
                    android:layout_gravity="center"
                    android:layout_marginStart="@dimen/_85pxh"
                    android:layout_marginEnd="@dimen/_57pxh"
                    android:layout_marginBottom="@dimen/_5pxv"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_vector_black_900"
                    tools:ignore="ContentDescription"
                    tools:src="@drawable/img_vector_black_900" />
            </LinearLayout>
        </FrameLayout>
