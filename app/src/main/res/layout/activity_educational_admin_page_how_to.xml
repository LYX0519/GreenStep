    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearColumngreenstep"
        android:layout_height="match_parent"
        android:layout_marginTop="@dimen/_4pxv"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylegreen_A100">
        <androidx.appcompat.widget.Toolbar
            android:gravity="center_horizontal"
            android:id="@+id/toolbarToolbar"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_13pxh"
            android:layout_marginStart="@dimen/_19pxh"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <LinearLayout android:id="@+id/linearRowdownload"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <LinearLayout android:id="@+id/linearRowvector"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.77"
                    android:layout_width="0dp" android:orientation="horizontal">
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearColumnvector"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_39pxv"
                        android:layout_marginTop="@dimen/_36pxv"
                        android:layout_weight="0.55"
                        android:layout_width="0dp" android:orientation="vertical">
                        <View
                            android:background="@drawable/rectangle_bg_gray_900_radius_1"
                            android:id="@+id/viewVector"
                            android:layout_height="@dimen/_2pxv"
                            android:layout_marginEnd="@dimen/_10pxh" android:layout_width="@dimen/_24pxh"/>
                        <View
                            android:background="@drawable/rectangle_bg_gray_900_radius_1"
                            android:id="@+id/viewVectorOne"
                            android:layout_height="@dimen/_2pxv"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_marginTop="@dimen/_3pxv" android:layout_width="@dimen/_24pxh"/>
                        <View
                            android:background="@drawable/rectangle_bg_gray_900_radius_1"
                            android:id="@+id/viewVectorTwo"
                            android:layout_height="@dimen/_2pxv"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_marginTop="@dimen/_3pxv" android:layout_width="@dimen/_24pxh"/>
                    </LinearLayout>
                    <FrameLayout android:id="@+id/frameLogoGreenStep"
                        android:layout_height="@dimen/_88pxv" android:layout_width="@dimen/_92pxh">
                        <TextView android:ellipsize="end"
                            android:gravity="top|start"
                            android:id="@+id/txtGreenStep"
                            android:layout_gravity="bottom|start"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_10pxv"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@{educationalAdminPageHowToVM.educationalAdminPageHowToModel.txtGreenStep}"
                            style="@style/txtQuicksandbold18" tools:text="@string/app_name"/>
                        <ImageView
                            android:id="@+id/imageTwoHundredFiftyTwo"
                            android:layout_gravity="top|center"
                            android:layout_height="@dimen/_77pxh"
                            android:layout_marginBottom="@dimen/_10pxv"
                            android:layout_marginEnd="@dimen/_7pxh"
                            android:layout_marginStart="@dimen/_8pxh"
                            android:layout_width="@dimen/_77pxh"
                            android:scaleType="fitXY"
                            imageRes="@{@drawable/img_252}"
                            tools:ignore="ContentDescription" tools:src="@drawable/logo_app"/>
                    </FrameLayout>
                </LinearLayout>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearColumndownload"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_23pxv"
                    android:layout_marginStart="@dimen/_57pxh"
                    android:layout_marginTop="@dimen/_21pxv"
                    android:layout_weight="0.23"
                    android:layout_width="0dp" android:orientation="vertical">
                    <ImageView android:id="@+id/imageDownload"
                        android:layout_height="@dimen/_13pxh"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginStart="@dimen/_5pxh"
                        android:layout_width="@dimen/_17pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_download"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_download"/>
                    <ImageView android:gravity="center_horizontal"
                        android:id="@+id/imageVolume"
                        android:layout_height="@dimen/_27pxh"
                        android:layout_marginTop="@dimen/_2pxv"
                        android:layout_width="match_parent"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_volume"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_volume"/>
                </LinearLayout>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearColumn"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_8pxv"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylegreen_A100">
            <LinearLayout android:gravity="start"
                android:id="@+id/linearColumnlanguage"
                android:layout_height="match_parent"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylewhite_A700cornerRadius">
                <TextView android:ellipsize="end"
                    android:gravity="center_horizontal"
                    android:id="@+id/txtLanguage"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_30pxh"
                    android:layout_marginStart="@dimen/_30pxh"
                    android:layout_marginTop="@dimen/_36pxv"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@{educationalAdminPageHowToVM.educationalAdminPageHowToModel.txtLanguage}"
                    style="@style/txtQuicksandbold20" tools:text="@string/msg_how_to_altern"/>
                <View android:background="@color/bluegray_300"
                    android:id="@+id/lineLineEleven"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_1pxv"
                    android:layout_marginEnd="@dimen/_29pxh"
                    android:layout_marginStart="@dimen/_30pxh"
                    android:layout_marginTop="@dimen/_4pxv" android:layout_width="@dimen/_300pxh"/>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearRowhowto"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_40pxh"
                    android:layout_marginStart="@dimen/_40pxh"
                    android:layout_marginTop="@dimen/_12pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtHowTo"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.57"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{educationalAdminPageHowToVM.educationalAdminPageHowToModel.txtHowTo}"
                        style="@style/txt" textUnderline="true" tools:text="@string/lbl_how_to"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtAlternatives"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_3pxv"
                        android:layout_marginStart="@dimen/_16pxh"
                        android:layout_marginTop="@dimen/_3pxv"
                        android:layout_weight="0.43"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{educationalAdminPageHowToVM.educationalAdminPageHowToModel.txtAlternatives}"
                        style="@style/txtQuicksandbold18_1" tools:text="@string/lbl_alternatives"/>
                </LinearLayout>
                <FrameLayout android:id="@+id/frameStacktitle"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_80pxv"
                    android:layout_marginEnd="@dimen/_30pxh"
                    android:layout_marginStart="@dimen/_30pxh"
                    android:layout_marginTop="@dimen/_9pxv"
                    android:layout_width="@dimen/_300pxh" style="@style/groupStylecornerRadius_1">
                    <LinearLayout android:id="@+id/linearColumntitle"
                        android:layout_gravity="bottom|start"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_width="match_parent"
                        android:orientation="vertical" style="@style/groupStylecornerRadius_1">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtTitle"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@{educationalAdminPageHowToVM.educationalAdminPageHowToModel.txtTitle}"
                            style="@style/txtQuicksandbold16" tools:text="@string/lbl_title"/>
                        <EditText
                            android:background="@drawable/rectangle_bg_light_green_50_border_green_902_radius_10"
                            android:ellipsize="end"
                            android:id="@+id/etRectangle4180"
                            android:inputType="text"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_3pxv"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@={educationalAdminPageHowToVM.educationalAdminPageHowToModel.etRectangle4180Value}"
                            style="@style/etSolidRoundedOutline" tools:ignore="Autofill"/>
                    </LinearLayout>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtOne"
                        android:layout_gravity="top|start"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:layout_marginEnd="@dimen/_43pxh"
                        android:layout_marginStart="@dimen/_43pxh"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{educationalAdminPageHowToVM.educationalAdminPageHowToModel.txtOne}"
                        style="@style/txtQuicksandbold25" tools:text="@string/lbl"/>
                </FrameLayout>
                <FrameLayout android:id="@+id/frameStacksourcereferen"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_82pxv"
                    android:layout_marginEnd="@dimen/_29pxh"
                    android:layout_marginStart="@dimen/_30pxh"
                    android:layout_marginTop="@dimen/_17pxv"
                    android:layout_width="@dimen/_300pxh" style="@style/groupStylecornerRadius_1">
                    <LinearLayout
                        android:id="@+id/linearColumnsourcereferen"
                        android:layout_gravity="bottom|start"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_width="match_parent"
                        android:orientation="vertical" style="@style/groupStylecornerRadius_1">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtSourceReferen"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@{educationalAdminPageHowToVM.educationalAdminPageHowToModel.txtSourceReferen}"
                            style="@style/txtQuicksandbold16" tools:text="@string/msg_source_referen"/>
                        <EditText
                            android:background="@drawable/rectangle_bg_light_green_50_border_green_902_radius_10"
                            android:ellipsize="end"
                            android:id="@+id/etRectangle4182"
                            android:inputType="text"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_3pxv"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@={educationalAdminPageHowToVM.educationalAdminPageHowToModel.etRectangle4182Value}"
                            style="@style/etSolidRoundedOutline" tools:ignore="Autofill"/>
                    </LinearLayout>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtTwo"
                        android:layout_gravity="top|end"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:layout_marginEnd="@dimen/_138pxh"
                        android:layout_marginStart="@dimen/_138pxh"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{educationalAdminPageHowToVM.educationalAdminPageHowToModel.txtTwo}"
                        style="@style/txtQuicksandbold25" tools:text="@string/lbl"/>
                </FrameLayout>
                <FrameLayout android:id="@+id/frameStackthree"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_202pxv"
                    android:layout_marginEnd="@dimen/_30pxh"
                    android:layout_marginStart="@dimen/_30pxh"
                    android:layout_marginTop="@dimen/_8pxv" android:layout_width="@dimen/_294pxh">
                    <LinearLayout android:id="@+id/linearColumnupload"
                        android:layout_gravity="bottom|start"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_width="match_parent" android:orientation="vertical">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtImageOne"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_marginStart="@dimen/_1pxh"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@{educationalAdminPageHowToVM.educationalAdminPageHowToModel.txtImageOne}"
                            style="@style/txtQuicksandbold16" tools:text="@string/lbl_image"/>
                        <FrameLayout android:id="@+id/frameStackupload"
                            android:layout_height="@dimen/_168pxv"
                            android:layout_marginTop="@dimen/_5pxv"
                            android:layout_width="@dimen/_294pxh" style="@style/groupStylelight_green_50cornerRadius">
                            <ImageView android:id="@+id/imageUpload"
                                android:layout_gravity="top|center"
                                android:layout_height="@dimen/_19pxh"
                                android:layout_marginBottom="@dimen/_68pxv"
                                android:layout_marginEnd="@dimen/_135pxh"
                                android:layout_marginStart="@dimen/_135pxh"
                                android:layout_marginTop="@dimen/_68pxv"
                                android:layout_width="@dimen/_20pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_upload"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_upload"/>
                        </FrameLayout>
                    </LinearLayout>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtThree"
                        android:layout_gravity="top|start"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:layout_marginEnd="@dimen/_60pxh"
                        android:layout_marginStart="@dimen/_60pxh"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{educationalAdminPageHowToVM.educationalAdminPageHowToModel.txtThree}"
                        style="@style/txtQuicksandbold25" tools:text="@string/lbl"/>
                </FrameLayout>
                <androidx.appcompat.widget.AppCompatButton
                    android:ellipsize="end" android:gravity="center"
                    android:id="@+id/btnComplete"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_30pxh"
                    android:layout_marginStart="@dimen/_30pxh"
                    android:layout_marginTop="@dimen/_55pxv"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_10pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_30pxh"
                    android:paddingTop="@dimen/_10pxv"
                    android:singleLine="true"
                    android:text="@string/lbl_complete"
                    android:textAllCaps="false"
                    style="@style/btnSolidRounded_2" tools:text="@string/lbl_complete"/>
                <ImageView android:id="@+id/imageVectorSeventyOne"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_3pxh"
                    android:layout_marginBottom="@dimen/_20pxv"
                    android:layout_marginEnd="@dimen/_30pxh"
                    android:layout_marginStart="@dimen/_30pxh"
                    android:layout_marginTop="@dimen/_47pxv"
                    android:layout_width="@dimen/_7pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_vector71"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_vector71"/>
            </LinearLayout>
        </LinearLayout>
        <FrameLayout android:gravity="start"
